<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.myapp783269" version="0.0.1" xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap="http://cordova.apache.org/ns/1.0">
    <name>ATAFAQ</name>
    <description>
        An Ionic Framework and Cordova project.
    </description>
    <author email="you@example.com" href="http://example.com.com/">
        Your Name Here
    </author>
    <content src="index.html"/>
    <access origin="*"/>
    <preference name="webviewbounce" value="false"/>
    <preference name="UIWebViewBounce" value="false"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="SplashScreenDelay" value="5000"/>
    <preference name="FadeSplashScreenDuration" value="2000"/>
    <preference name="SplashScreen" value="screen"/>
    <preference name="android-minSdkVersion" value="16"/>
    <preference name="BackupWebStorage" value="none"/>
    <preference name="CrosswalkAnimatable" value="true"/>
    <!-- plugin -->
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
    <plugin name="cordova-plugin-whitelist" spec="~1.2.2"/>
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2"/>
    <plugin name="cordova-plugin-inappbrowser"/>
    <plugin name="cordova-plugin-camera"/>
    <plugin name="cordova-plugin-media-capture"/>
    <plugin name="cordova-plugin-file-transfer"/>
    <plugin name="cordova-plugin-geolocation"/>
    <plugin name="cordova.plugins.diagnostic" source="npm"/>
    <plugin name="cordova-plugin-request-location-accuracy" source="npm"/>
    <!--<plugin name="https://github.com/VersoSolutions/CordovaClipboard"  />-->
    <plugin name="cordova-plugin-x-socialsharing" source="npm"/>
    <feature name="SocialSharing">
        <param name="ios-package" value="SocialSharing"/>
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing"/>
    </feature>

    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" onload="true"/>
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="cordova-plugin-inappbrowser"/>
    </feature>

    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser"/>
    </feature>

    <feature name="InAppBrowser"/>
    <!-- Android icons -->
    <icon src="resources/Android/res/drawable/icon.png"/>
    <icon src="resources/Android/res/drawable-ldpi/icon.png" gap:platform="android" gap:qualifier="ldpi"/>
    <icon src="resources/Android/res/drawable-mdpi/icon.png" gap:platform="android" gap:qualifier="mdpi"/>
    <icon src="resources/Android/res/drawable-hdpi/icon.png" gap:platform="android" gap:qualifier="hdpi"/>
    <icon src="resources/Android/res/drawable-xhdpi/icon.png" gap:platform="android" gap:qualifier="xhdpi"/>
    <icon src="resources/Android/res/drawable-xxhdpi/icon.png" gap:platform="android" gap:qualifier="xxhdpi"/>
    <icon src="resources/Android/res/drawable-xxxhdpi/icon.png" gap:platform="android" gap:qualifier="xxxhdpi"/>
    <!-- iOs icon  -->
    <icon src="resources/iOS/Resources/icons/Icon-small.png" gap:platform="ios" width="29" height="29"/>
    <icon src="resources/iOS/Resources/icons/Icon.png" gap:platform="ios" width="57" height="57"/>
    <icon src="resources/iOS/Resources/icons/Icon@2x.png" gap:platform="ios" width="114" height="114"/>
    <icon src="resources/iOS/Resources/icons/Icon-40.png" gap:platform="ios" width="40" height="40"/>
    <icon src="resources/iOS/Resources/icons/Icon-40@2x.png" gap:platform="ios" width="80" height="80"/>
    <icon src="resources/iOS/Resources/icons/Icon-50.png" gap:platform="ios" width="50" height="50"/>
    <icon src="resources/iOS/Resources/icons/Icon-50@2x.png" gap:platform="ios" width="58" height="58"/>
    <icon src="resources/iOS/Resources/icons/Icon-100.png" gap:platform="ios" width="100" height="100"/>
    <icon src="resources/iOS/Resources/icons/Icon-60.png" gap:platform="ios" width="60" height="60"/>
    <icon src="resources/iOS/Resources/icons/Icon-72.png" gap:platform="ios" width="72" height="72"/>
    <icon src="resources/iOS/Resources/icons/Icon-72@2x.png" gap:platform="ios" width="144" height="144"/>
    <icon src="resources/iOS/Resources/icons/Icon-76.png" gap:platform="ios" width="76" height="76"/>
    <icon src="resources/iOS/Resources/icons/Icon-76@2x.png" gap:platform="ios" width="80" height="80"/>
    <icon src="resources/iOS/Resources/icons/Icon-167.png" gap:platform="ios" width="120" height="120"/>
    <icon src="resources/iOS/Resources/icons/Icon-small@2x.png" gap:platform="ios" width="152" height="152"/>
    <icon src="resources/iOS/Resources/icons/Icon-small@3x.png" gap:platform="ios" width="180" height="180"/>

    <gap:splash gap:platform="android" gap:qualifier="port-ldpi"
                src="resources/Android/splash/drawable-ldpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi"
                src="resources/android/splash/drawable-mdpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi"
                src="resources/android/splash/drawable-hdpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi"
                src="resources/android/splash/drawable-xhdpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="land-ldpi"
                src="resources/Android/splash/drawable-land-ldpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="land-mdpi"
                src="resources/android/splash/drawable-land-mdpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="land-hdpi"
                src="resources/android/splash/drawable-land-hdpi/screen.png"/>
    <gap:splash gap:platform="android" gap:qualifier="land-xhdpi"
                src="resources/android/splash/drawable-land-xhdpi/screen.png"/>

    <!--    ios Splash Screen-->
    <gap:splash src="resources/iOS/splash/Default~iphone.png" gap:platform="ios" width="320" height="480"/>
    <gap:splash src="resources/ios/splash/Default@2x~iphone_640x960.png" gap:platform="ios" width="640" height="960"/>
    <gap:splash src="resources/ios/splash/Default-568h@2x~iphone_640x1136.png" gap:platform="ios" width="640"
                height="1136"/>
